{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": [],
  "compilerOptions": {
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": "src"
  },
  "include": [
    "next-env.d.ts",
    "**/*.tsx",
    "**/*.ts",
    "**/*.d.ts",
    ".next/types/**/*.ts"
  ],
  "exclude": ["node_modules", ".next"]
}

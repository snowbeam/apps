generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  sys_id String @id @default(cuid()) @map("sys_id") @db.VarChar(64)

  @@map("sys_user")
}

model Roles {
  sys_id String @id @default(cuid()) @map("sys_id") @db.VarChar(64)

  @@map("sys_role")
}

model UserHasRoles {
  sys_id String @id @default(cuid()) @map("sys_id") @db.VarChar(64)

  @@map("sys_user_has_role")
}

model Groups {
  sys_id String @id @default(cuid()) @map("sys_id") @db.VarChar(64)

  @@map("sys_group")
}

model Tables {
  sys_id String @id @default(cuid()) @map("sys_id") @db.VarChar(64)

  @@map("sys_db_object")
}

model SystemProperties {
  sys_id String @id @default(cuid()) @map("sys_id") @db.VarChar(64)

  @@map("sys_properties")
}

model WorkspaceSchema {
  sys_id       String @id @default(cuid()) @map("sys_id") @db.VarChar(64)
  workspaceXml String @map("workspace_xml")

  @@map("workspace_schema")
}
